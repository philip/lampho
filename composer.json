{
    "name": "philip/lampho",
    "description": "Lampho is a test project with a horrible name that's like lambo written in PHP",
    "keywords": ["laravel", "console"],
    "homepage": "https://github.com/philip/lampho",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/philip/lampho/issues",
        "source": "https://github.com/philip/lampho"
    },
    "authors": [
        {
            "name": "Philip Olson",
            "email": "philip@roshambo.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["lampho"]
}
